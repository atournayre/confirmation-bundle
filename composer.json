{
    "description": "Confirmation bundle",
    "name": "atournayre/confirmation-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.11",
        "symfony/config": "6.2.*",
        "symfony/dependency-injection": "6.2.*",
        "symfony/form": "6.2.*",
        "symfony/http-foundation": "6.2.*",
        "symfony/routing": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "6.2.*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Atournayre\\Bundle\\ConfirmationBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atournayre\\Bundle\\ConfirmationBundle\\Tests\\": "tests/" }
    }
}
