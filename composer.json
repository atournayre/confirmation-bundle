{
    "description": "Confirmation bundle",
    "name": "atournayre/confirmation-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "atournayre/helpers": "^1.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.11",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/uid": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4 || ^6.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Atournayre\\Bundle\\ConfirmationBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atournayre\\Bundle\\ConfirmationBundle\\Tests\\": "tests/" }
    }
}
